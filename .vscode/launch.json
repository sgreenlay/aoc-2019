{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(Windows) Launch",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceRoot}/target/debug/advent_of_code_2019.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    "name": "RUST_BACKTRACE",
                    "value" : "1",
                }
            ],
            "externalConsole": false,
            "logging": {
                "exceptions": true,
                "moduleLoad": false,
                "programOutput": true,
                "engineLogging": false,
                "trace": true,
                "traceResponse": false,
            },
        }
    ]
}